pipeline {
  agent any
    stages {
      stage('BUild Application'){
        steps {
          echo "this is a build package "
       }
     }
    stage('build'){
       steps {
        echo "we are learining build, building the code nowe"
       }
     }
     stage('deploy'){
       steps {
        echo "we are deploying the app"
       }
     }
}
}
